{
  "kind": "collectionType",
  "collectionName": "blog_categories",
  "info": {
    "singularName": "blog-category",
    "pluralName": "blog-categories",
    "displayName": "Blog Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "order": {
      "type": "integer",
      "default": 0
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "inspirational_stories": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
